{
  "name": "node-measured-es",
  "author": "Felix Geisend√∂rfer <felix@debuggable.com> (http://debuggable.com/)",
  "description": "This is an alternative port of Coda Hale's metrics library.",
  "version": "2.0.1",
  "homepage": "https://github.com/felixge/node-measured",
  "engines": {
    "node": ">= 0.10"
  },
  "main": "./lib",
  "module": "./src",
  "scripts": {
    "build": "rm -rf ./lib && flow-remove-types -p -d ./lib/ ./src/",
    "test": "npm run build && mocha -r flow-remove-types/register -r @std/esm './test/unit/**/test-*.mjs'",
    "publish": "npm run test"
  },
  "devDependencies": {
    "@std/esm": "^0.19.7",
    "flow-remove-types": "^1.2.3",
    "mocha": "^2.1",
    "sinon": "^1.12"
  },
  "repository": {
    "url": "git://github.com/felixge/node-measured.git"
  },
  "files": [
    "index.js",
    "lib",
    "Readme.md",
    "CHANGES.md"
  ],
  "@std": {
    "esm": {
      "esm": "all",
      "cjs": true
    }
  },
  "license": "MIT"
}
